{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "version": 2,
  "functions": {
    "api/auth/register": {
      "runtime": "nodejs16.x",
      "memory": 1024
    }
  },

  "routes": [
    {
      "src": "/api/auth/register",
      "dest": "api/auth/register.ts"
    },
    {
      "src": "/api/auth/logout",
      "dest": "api/auth/logout.ts"
    },
    {
      "src": "/api/auth/login",
      "dest": "api/auth/login.ts"
    },
    {
      "src": "/api/user/me",
      "dest": "api/user/me.ts"
    }
  ],
  "env": {},
  "build": {
    "env": {
      "PRISMA_GENERATE": "true"
    }
  }
}
